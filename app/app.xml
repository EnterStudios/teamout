<?xml version="1.0" encoding="UTF-8" ?>
<Module>
	<ModulePrefs title="Hangjoy">
		<Require feature="rpc" />
		<Require feature="views" />
		<Require feature="locked-domain" />
	</ModulePrefs>
	<Content type="html"><![CDATA[
<!DOCTYPE html>
<html style="height: 100%" lang="en">
  <head>
    <link href="//s3.amazonaws.com/hangjoy/assets/css/bootstrap.css" rel="stylesheet">
    <link href="//s3.amazonaws.com/hangjoy/assets/css/bootstrap-theme.css" rel="stylesheet">
    <link href="//s3.amazonaws.com/hangjoy/assets/css/nanoscroller.css" rel="stylesheet">
    <link href="//s3.amazonaws.com/hangjoy/assets/css/introjs.css" rel="stylesheet">
    <link href="//s3.amazonaws.com/hangjoy/assets/css/app.css" rel="stylesheet">
  </head>
  <body style="height: 100%">
    <div class="menubar btn-group" data-toggle="buttons">
      <label class="btn btn-photo" data-toggle="tooltip" data-title-off="Turn snapshots off" data-title-on="Turn snapshots on">
        <input type="checkbox">
        <span class="glyphicon glyphicon-camera"></span>
      </label>
      <div class="btn btn-dropdown-container btn-photo-dropdown">
        <button type="button" class="btn btn-dropdown dropdown-toggle">
          <span class="glyphicon glyphicon-chevron-down"></span>
        </button>
        <div class="dropdown-menu">
          <div class="form-group setting-unmute_video checkbox">
            <label>
              <input type="checkbox"> Auto-unmute video
            </label>
          </div>
          <div class="form-group setting-photo_privacy">
            <label>Photo privacy:</label>
            <select class="form-control">
              <option value="none">None</option>
              <option value="blur">Blur</option>
              <option value="pixelate">Pixelate</option>
              <option value="silhouette">Presence Only</option>
            </select>
          </div>
          <div class="form-group setting-photo_interval">
            <label>Photo interval:</label>
            <select class="form-control">
              <option value="0.5">30 seconds</option>
              <option value="1">1 minute</option>
              <option value="2">2 minutes</option>
              <option value="3">3 minutes</option>
              <option value="4">4 minutes</option>
              <option value="5">5 minutes</option>
            </select>
          </div>
          <div class="form-group setting-photo_source">
            <label>Photo Source:</label>
            <select class="form-control">
            </select>
          </div>
        </div>
      </div>
      <label class="btn btn-available" data-toggle="tooltip" data-title-off="Show as busy" data-title-on="Show as available">
        <input type="checkbox">
        <span class="glyphicon glyphicon-exclamation-sign"></span>
      </label>
      <label class="btn btn-sounds" data-toggle="tooltip" data-title-off="Mute sounds" data-title-on="Unmute sounds">
        <input type="checkbox">
        <span class="glyphicon glyphicon-volume-off"></span>
      </label>
      <label class="btn btn-notifications" data-toggle="tooltip" data-title-off="Turn notifications off" data-title-on="Turn notifications on">
        <input type="checkbox">
        <span class="glyphicon glyphicon-bullhorn"></span>
      </label>
      <button type="button" class="btn btn-rooms" data-toggle="tooltip modal" data-target="#rooms" title="List rooms">
        <span class="glyphicon glyphicon-list"></span>
      </button>
      <button type="button" class="btn btn-leave disabled" data-toggle="tooltip" title="Leave conversation">Leave</button>
    </div>

    <div id="content" class="nano">
      <div class="nano-content">
        <div class="conversation">
          <span class="title glyphicon glyphicon-facetime-video"></span>
          <ul class="hanging_with list-group">
          </ul>
          <div class="actions">
            <a href="#" id="conversation-more" class="dropdown-toggle" data-toggle="dropdown"><span class="expand glyphicon glyphicon-list"></span></a>
            <ul class="conversation-names dropdown-menu dropdown-menu-right" role="menu" aria-labelledby="conversation-more">
            </ul>
          </div>
        </div>

        <div class="alert alert-info participants-empty">
          <span class="glyphicon glyphicon-user"></span>
          <span class="caption">There are no other participants in the hangout.</span>
        </div>

        <ul class="participants list-group">
        </ul>
      </div>
    </div>

    <div class="modal fade" id="rooms" tabindex="-1">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">Room directory</h4>
          </div>
          <div class="modal-body">
            <div class="form-group rooms-share">
              <label for="room-share">Link to share</label>
              <input type="email" class="form-control" id="room-share" readonly="readonly">
            </div>
            <div class="form-group rooms-other">
              <label>Other rooms</label>
              <span class="rooms-none">No rooms available</span>
              <ul class="nav nav-pills nav-stacked">
              </ul>
            </div>
            <div class="form-group rooms-create">
              <label for="room-create">Create new room</label>
              <input type="email" class="form-control" id="room-create" placeholder="new-room-id">
              <button type="submit" class="btn btn-default">Create</button>
            </div>
          </div>
        </div>
      </div>
    </div>

    <script type="text/javascript" src="//plus.google.com/hangouts/_/api/v1/hangout.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/jquery.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/bootstrap.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/jquery.nanoscroller.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/StackBlur.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/ccv.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/face.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/sha1.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/strftime.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.core.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.support.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.data.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.layout.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.notification.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.participant.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.participants.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.conversation.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.photo.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.avatar.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.rooms.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.settings.js"></script>
    <script type="text/javascript" src="//s3.amazonaws.com/hangjoy/assets/js/app.outro.js"></script>
  </body>
]]>
</Content>
</Module>